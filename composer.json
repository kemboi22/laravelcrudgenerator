{
    "name": "kemboielvis/laravel-crud-generator",
    "description": "A laravel CRUD generator",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kemboielvis\\LaravelCrudGenerator\\": "src/"
        }
    },
    "authors": [
        {
            "name": "kemboi22",
            "email": "kemboielvis22@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^11.0",
        "illuminate/database": "^11.0",
        "illuminate/console": "^11.0",
        "illuminate/filesystem": "^11.0",
        "illuminate/validation": "^11.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Kemboielvis\\LaravelCrudGenerator\\LaravelCrudServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^9.2"
    }
}
